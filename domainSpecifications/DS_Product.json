          "sh:or": [
            {
              "sh:datatype": "xsd:string"
            }
          ],
          "sh:minCount": 1,
          "sh:path": "schema:description"
        },
        {
          "@type": "sh:PropertyShape",
          "sh:or": [
            {
              "sh:datatype": "xsd:anyURI"
            }
          ],
          "sh:minCount": 1,
          "sh:maxCount": 1,
          "sh:path": "schema:url"
        }
