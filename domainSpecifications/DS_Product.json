    },
    "sh:in": {
      "@id": "sh:in",
      "@container": "@list"
    },
    "sh:languageIn": {
      "@id": "sh:languageIn",
      "@container": "@list"
    },
    "sh:equals": {
      "@id": "sh:equals",
      "@type": "@id"
    },
    "sh:disjoint": {
      "@id": "sh:disjoint",
      "@type": "@id"
    },
    "sh:lessThan": {
